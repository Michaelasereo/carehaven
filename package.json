{
  "name": "carehaven",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:clean": "rm -rf .next && npm run build",
    "build:optimized": "rm -rf .next && npm run build && rm -rf .next/cache .next/standalone .next/trace .next/types .next/**/*.map .next/**/node_modules",
    "build:netlify": "npm run build && rm -rf .next/cache .next/standalone .next/trace .next/types .next/**/*.map .next/**/node_modules",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "seed:doctors": "tsx scripts/seed-doctors.ts",
    "create:admins": "tsx scripts/create-admin-users.ts",
    "test:e2e": "tsx scripts/test-e2e-journey.ts",
    "test:auth": "tsx scripts/test-auth-system.ts",
    "test:verification": "tsx scripts/test-verification-email.ts",
    "seed:sample-session": "tsx scripts/seed-sample-session-data.ts",
    "clear:users": "tsx scripts/clear-users-except-admin.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:test": "tsx scripts/test-prisma.ts",
    "deploy:netlify": "bash scripts/deploy-netlify.sh",
    "deploy:netlify:direct": "npm run build && npx netlify-cli deploy --dir=.next --prod --functions=netlify/functions --timeout=600",
    "deploy:netlify:no-plugins": "NETLIFY_USE_PLUGINS=false npx netlify-cli deploy --prod --timeout=600",
    "deploy:netlify:standard": "npx netlify-cli deploy --prod --timeout=600",
    "deploy:netlify:safe": "npm run build && NETLIFY_RETRY_COUNT=5 NETLIFY_RETRY_TIMEOUT=300000 npx netlify-cli deploy --prod --timeout=300",
    "deploy:netlify:debug": "NETLIFY_DEBUG=true NETLIFY_LOG_LEVEL=debug npx netlify-cli deploy --prod --debug --timeout=600",
    "deploy:netlify:region-us": "NETLIFY_REGION=us npm run build && NETLIFY_REGION=us npx netlify-cli deploy --prod --timeout=600",
    "deploy:netlify:retry": "npm run build:optimized && NETLIFY_RETRY_COUNT=5 NETLIFY_RETRY_TIMEOUT=300000 NODE_OPTIONS='--max-http-header-size=16384' npx netlify-cli deploy --prod --timeout=300",
    "deploy:netlify:optimized": "npm run build:optimized && NETLIFY_CDN_UPLOAD_CONCURRENCY=2 NETLIFY_CDN_UPLOAD_TIMEOUT=300 NETLIFY_RETRY_COUNT=3 npx netlify-cli deploy --prod --timeout=300",
    "deploy:prod": "npx netlify-cli deploy --prod --timeout=600",
    "deploy:prod:standard": "NETLIFY_SDK_METADATA_URL=\"\" npx netlify-cli deploy --prod --timeout=600",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.78.0",
    "@getbrevo/brevo": "^1.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^6.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.0.6",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.400.0",
    "next": "^16.1.1",
    "prisma": "^6.2.1",
    "react": "^19.2.3",
    "react-big-calendar": "^1.8.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.49.3",
    "recharts": "^2.10.3",
    "tailwind-merge": "^2.6.0",
    "twilio": "^4.20.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@netlify/functions": "^5.1.2",
    "@netlify/plugin-nextjs": "^5.15.5",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "typescript": "5.9.3"
  }
}
